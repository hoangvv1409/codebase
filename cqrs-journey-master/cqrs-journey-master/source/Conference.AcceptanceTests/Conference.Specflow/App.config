    <add name="DbContext.SqlBus" connectionString="Data Source=.\sqlexpress;Initial Catalog=conference;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="DbContext.ConferenceManagement" connectionString="Data Source=.\sqlexpress;Initial Catalog=conference;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <entityFramework>
